<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lgyun.system.user.mapper.ServiceProviderWorkerMapper">

    <resultMap type="com.lgyun.system.user.vo.admin.QueryServiceProviderWorkerListVO" id="queryServiceProviderWorkerListVO">
        <result property="serviceProviderId" column="service_provider_id"/>
        <result property="workerName" column="worker_name"/>
        <result property="phoneNumber" column="phone_number"/>
    </resultMap>

    <select id="queryServiceProviderWorkerList" resultMap="queryServiceProviderWorkerListVO">
        SELECT id AS service_provider_id, worker_name, phone_number FROM diyi_service_provider_worker WHERE is_deleted = 0
        AND service_provider_id = #{param1} AND position_name = #{param2}
    </select>

</mapper>
